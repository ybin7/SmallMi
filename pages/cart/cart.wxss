/* pages/cart/cart.wxss */

.empty-cart {
  display: flex;
  flex-direction: column;
  align-items: center;
  background-color: #fff;
  padding-bottom: 4rem;
}

.empty-cart .cart-icon {
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
  width: 190rpx;
  height: 190rpx;
  margin-top: 100rpx;
  border-radius: 50%;
  background: #d6d6d6;
}

.cart-icon .bgImg {
  position: absolute;
  width: 16.29333rem;
  height: 6.73333rem;
  background: transparent url(data:image/png;base64,);
  left: 50%;
  -webkit-transform: translateX(-50%);
  transform: translateX(-50%);
  background-size: 100%;
  top: 0;
  margin-top: 10rpx;
}

.cart-icon image {
  width: 80rpx;
  height: 80rpx;
}

.btn-warn, .prompt {
  margin-top: 80rpx;
}

.btn-warn {
  width: 580rpx;
  letter-spacing: 6rpx;
}

/*购物车结算操作*/

.user-operation {
  width: 100%;
  height: 100rpx;
  line-height: 100rpx;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
  position: fixed;
  left: 0;
  bottom: 0;
}

.total-price {
  flex: 2;
  font-size: 40rpx;
  text-align: center;
  margin-left: 20rpx;
}

.select-all text {
  margin-left: 70rpx;
}

.select-all, .btn-primary.pay {
  flex: 1;
  font-size: 40rpx;
  text-align: center;
}

.select-all icon {
  position: absolute;
  left: 12rpx;
  top: 26rpx;
}

.total-price text {
  font-size: 12pt;
  color: #f44336;
  margin-left: -14rpx;
}

.btn-primary.pay {
  flex: 1;
  font-size: 12pt;
  color: #fff;
  background: #f44336;
}

/*购物车列表样式*/

.cart-box {
  width: 100%;
  margin-top: 12rpx;
  background: #fff;
}

.cart-list {
  width: 100%;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}

.cart-item {
  height: 150rpx;
  width: 100%;
  display: flex;
  flex-direction: row;
  justify-content: space-around;
  align-items: center;
  font-size: 11pt;
  border-top: 1rpx solid #dcdcdc;
}

.cart-item icon {
  margin-left: -140rpx;
}

.cart-item text {
  display: inline-block;
  margin-left: 10rpx;
  font-size:30rpx;
}

.cart-item .item-price {
  color: #f44336;
}

.cart-list image {
  width: 60rpx;
  height: 80rpx;
  display: inline-block;
}

.del-cart-item {
  background-color: #f44336;
  width: 150rpx;
  height: 150rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  color: #fff;
  -webkit-transform: translateX(150rpx);
  transform: translateX(150rpx);
  -webkit-transition: all 0.4s;
  transition: all 0.4s;
}

.cart-content {
  width: 100%;
  display: flex;
  flex-direction: row;
  justify-content: space-around;
  align-items: center;
  margin-right: -150rpx;
  -webkit-transition: all 0.4s;
  transition: all 0.4s;
  -webkit-transform: translateX(150rpx);
  transform: translateX(150rpx);
  margin-left: -150rpx;
}

.touch-move-active .cart-content, .touch-move-active .del-cart-item {
  -webkit-transform: translateX(0);
  transform: translateX(0);
}
